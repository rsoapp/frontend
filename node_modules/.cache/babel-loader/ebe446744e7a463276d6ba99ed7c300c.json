se) => {\n      this.ad = response.data;\n      this.images = this.ad.adImagesDto.images;\n      this.imagesLoaded = true\n    }).catch(e => {\n      console.log('Error', e)\n    })\n  }\n}\n</script>\n\n<style scoped>\n.sendEmailButton {\n  display: inline-block;\n  float: none;\n  vertical-align: top;\n  text-align: center;\n  margin-top: 10px;\n}\n\n.adImage {\n  max-width: 350px !important;\n  height: auto;\n}\n\n.image {\n  margin: 5px;\n}\n\n.infoDiv {\n    width: 500px;\n}\n</style>"],"sourceRoot":"src/views"}]}